        const price = await this.arweave.transactions.getPrice(data.length);
        return this.arweave.createTransaction({
            data: data,
            reward: price
        }, key);
    }
    async newSiloTransaction(key, data, siloDestination) {
        const siloReference = new silo_1.SiloReference(siloDestination);
        const encryptedData = keys.encrypt(Buffer.from(data), siloReference.getEncryptionKey()).toString();
        const price = await this.arweave.transactions.getPrice(encryptedData.length);
        const transaction = await this.arweave.createTransaction({
            data: encryptedData,
            reward: price
        }, key);
        transaction.addTag('Silo-Name', siloReference.getName());
        return transaction;
    }
