/***/ "./src/lib/file.ts":
/*!*************************!*\
  !*** ./src/lib/file.ts ***!
  \*************************/
/*! no static exports found */
/***/ (function(module, exports, __webpack_require__) {

"use strict";

Object.defineProperty(exports, "__esModule", { value: true });
const fs = __webpack_require__(/*! fs */ "fs");
const path = __webpack_require__(/*! path */ "path");
class File {
    constructor(path, base) {
        this.path = path;
        this.base = base;
    }
    getPath() {
        return this.base ? path.resolve(this.base, this.path) : this.path;
    }
    read(options) {
        return new Promise((resolve, reject) => {
            fs.readFile(this.getPath(), options, (error, data) => {
                if (error) {
                    reject(error);
                }
                resolve(data);
            });
        });
    }
    write(data, options) {
        return new Promise((resolve, reject) => {
            fs.writeFile(this.getPath(), data, options, (error) => {
                if (error) {
                    reject(error);
                }
                resolve();
            });
        });
    }
    exists() {
        return new Promise((resolve, reject) => {
            fs.exists(this.getPath(), (exists) => {
                resolve(exists);
            });
        });
    }
    info() {
        return new Promise((resolve, reject) => {
            fs.stat(this.getPath(), (error, stats) => {
                if (error) {
                    reject(error);
                }
                resolve(stats);
            });
        });
    }
    delete() {
        return new Promise((resolve, reject) => {
            fs.unlink(this.getPath(), (error) => {
                if (error) {
                    reject(error);
                }
                resolve();
            });
        });
    }
    static bytesForHumans(bytes) {
        const sizes = [
            'Bytes', 'kB', 'MB', 'GB', 'TB', 'PB', 'EB'
        ];
        let output;
        sizes.forEach((unit, id) => {
            const s = Math.pow(1024, id);
            let fixed = '';
            if (bytes >= s) {
                fixed = String((bytes / s).toFixed(2));
                if (fixed.indexOf('.0') === fixed.length - 2) {
                    fixed = fixed.slice(0, -2);
                }
                output = `${fixed} ${unit}`;
            }
        });
        if (!output) {
            `0 Bytes`;
        }
        return output;
    }
}
exports.File = File;


